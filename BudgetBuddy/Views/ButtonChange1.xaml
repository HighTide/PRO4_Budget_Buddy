<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BudgetBuddy.Views.ButtonChange1"
             Title="Change button1">
    <ContentPage.Content>
        <StackLayout>
            <Button BorderColor="Bisque" Margin="5" BorderWidth="2" Text="Button1 to Inkomsten" Clicked="Button_OnClicked1" TextColor="{DynamicResource textColor}"  BackgroundColor="{DynamicResource buttonColor}"></Button>
            <Button BorderColor="Bisque" Margin="5" BorderWidth="2" Text="Button1 to Uitgaven" Clicked="Button_OnClicked2" TextColor="{DynamicResource textColor}"  BackgroundColor="{DynamicResource buttonColor}"></Button>
            <Button BorderColor="Bisque" Margin="5" BorderWidth="2" Text="Button1 to Spaardoelen" Clicked="Button_OnClicked3" TextColor="{DynamicResource textColor}"  BackgroundColor="{DynamicResource buttonColor}"></Button>
            <Button BorderColor="Bisque" Margin="5" BorderWidth="2" Text="Button1 to Overzicht" Clicked="Button_OnClicked4" TextColor="{DynamicResource textColor}"  BackgroundColor="{DynamicResource buttonColor}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>